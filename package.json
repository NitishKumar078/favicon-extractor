{
  "name": "favicon-extractor",
  "version": "1.1.4",
  "description": "A lightweight Node.js utility to fetch and extract favicons from any website URL. Perfect for developers who need quick access to site icons for apps, extensions, or APIs.",
  "keywords": [
    "favicon",
    "icon",
    "logo",
    "website",
    "logo"
  ],
  "homepage": "https://github.com/NitishKumar078/favicon-extractor#readme",
  "bugs": {
    "url": "https://github.com/NitishKumar078/favicon-extractor/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NitishKumar078/favicon-extractor.git"
  },
  "license": "MIT",
  "author": "Nitish kumar M",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "pretest": "npm run build",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
    "test:coverage": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage",
    "build": "tsc",
    "start": "node dist/index.js",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "axios": "^1.12.0",
    "cheerio": "^1.1.2"
  },
  "devDependencies": {
    "jest": "^30.1.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  }
}
